set ylabel "loadavg"
plot "/root/cametrics/test_uc1-20090525T011554/ibolmo.com_monitor.data" u 1:3 t "CPU 1=100%" w impulse lw 2 lt 1, "" u 1:4 t "Load 1min" w lines lw 2 lt 3, "" u 1:5 t "Load 5min" w lines lw 2 lt 4, "" u 1:6 t "Load 15min" w lines lw 2 lt 5
set title "Network traffic"
set ylabel "kB/s"
plot "/root/cametrics/test_uc1-20090525T011554/ibolmo.com_monitor.data" u 1:8 t "In" w lines lw 2 lt 2, "" u 1:9 t "Out" w lines lw 1 lt 1
set title "Memory usage"
set ylabel "kB"
plot "/root/cametrics/test_uc1-20090525T011554/ibolmo.com_monitor.data" u 1:7 t "Memory" w lines lw 2 lt 2, "" u 1:8 t "Swap" w lines lw 2 lt 1
unset multiplot
