set output "/root/cametrics/test_uc1-20090525T011554/ibolmo.com_monitor.png"
set terminal png size 640,768
set multiplot layout 4, 1 title "Monitoring ibolmo.com"
set grid back
set xdata time
set timefmt "%H:%M:%S"
set format x "%H:%M"
